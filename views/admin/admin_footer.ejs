<script>
  let getSubcatData = () => {
    let catData = document.getElementById("Category").value;
    $.ajax({
      url: "/admin/extraCategory/getSubcategory",
      method: "post",
      data: {
        catData: catData
      },
      success: (res) => {
        $("#Subcategory").html(res);
      }
    })
  };

  let getExtraCatData = () => {
    let catData = document.getElementById("Category").value;
    let subcatData = document.getElementById("Subcategory").value;
    $.ajax({
      url: "/admin/extraCategory/getExtraCategory",
      method: "post",
      data: {
        catData: catData,
        subcatData: subcatData
      },
      success: (res) => {
        $("#ExtraCategory").html(res);
      }
    })
  };

  let getBrandTypeData = () => {
    let catData = document.getElementById("Category").value;
    let subcatData = document.getElementById("Subcategory").value;
    let extraCatData = document.getElementById("ExtraCategory").value;
    $.ajax({
      url: "/admin/extraCategory/getBrandType",
      method: "post",
      data: {
        catData: catData,
        subcatData: subcatData,
        extraCatData: extraCatData
      },
      success: (res) => {
        $("#BrandType").html(res);
      }
    })
  };
  
  $(document).ready(function () {
      $("#selectAll").click(function () {
          $(".checkboxSub").attr('checked', this.checked);
      });
  });
</script>